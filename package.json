{
  "name": "@seungmanchoi/app-publisher-mcp",
  "version": "1.0.0",
  "description": "MCP server for vibe coders - AI app icon generation, auto-resizing for iOS/Android, and store publishing automation",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "app-publisher-mcp": "dist/index.js"
  },
  "scripts": {
