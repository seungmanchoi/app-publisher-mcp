  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seungmanchoi/app-publisher-mcp.git"
  },
  "homepage": "https://github.com/seungmanchoi/app-publisher-mcp#readme",
  "bugs": {
    "url": "https://github.com/seungmanchoi/app-publisher-mcp/issues"
  },
  "dependencies": {
    "@google/genai": "^1.0.0",
    "@modelcontextprotocol/sdk": "^1.12.1",
    "sharp": "^0.33.0",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@types/node": "^22.0.0",
    "eslint": "^9.0.0",
    "typescript": "^5.7.0",
    "typescript-eslint": "^8.0.0"
  }
